generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model MediaMetadata {
  id        Int      @id @default(autoincrement())
  userId    String
  filename  String
  url       String
  filetype  String
  uploaded  DateTime @default(now())
}

model User {
  id    String @id
  email String
  name  String
}

